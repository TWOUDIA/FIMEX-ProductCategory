<?xml version='1.0' encoding='utf-8'?>
<widget defaultlocale="en-US" id="com.twoudia.FIMEX2016WPAPIproduct" version="0.1.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
    <access origin="*" />
    <feature name="Console">
        <param name="wp-package" value="DebugConsole" />
    </feature>
    <feature name="Device">
        <param name="wp-package" value="Device" />
    </feature>
    <feature name="StatusBar">
        <param name="wp-package" value="StatusBar" />
    </feature>
    <icon height="99" src="res/icons/wp8/ApplicationIcon.png" width="99" />
    <icon height="159" src="res/icons/wp8/Background.png" width="159" />
    <splash height="1280" src="res/screens/wp8/SplashScreenImage.png" width="768" />
    <name>FIMEX PRODUCT</name>
    <description>Multiple Platform APPs using Apache Cordova + WordPress, w/ IONbuilder.</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">TWOUDIA-YANNICK</author>
    <content src="index.html" />
    <vs:features />
    <preference name="SplashScreen" value="screen" />
    <preference name="windows-target-version" value="8.1" />
    <preference name="windows-phone-target-version" value="8.1" />
    <preference name="Orientation" value="portrait" />
    <preference name="Fullscreen" value="False" />
    <preference name="target-device" value="universal" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashScreenDelay" value="2000" />
    <preference name="KeyboardShrinksView" value="true" />
    <icon src="res/icons/android/drawable-xhdpi-icon.png" />
    <preference name="KeepRunning" value="True" />
    <preference name="ShowTitle" value="True" />
    <preference name="InAppBrowserStorageEnabled" value="True" />
    <preference name="SuppressesIncrementalRendering" value="True" />
    <preference name="deployment-target" value="7.0" />
    <vs:plugin name="cordova-plugin-device" version="1.1.0" />
    <vs:plugin name="cordova-plugin-statusbar" version="2.0.0" />
    <vs:plugin name="cordova-plugin-console" version="1.0.2" />
    <vs:plugin name="ionic-plugin-keyboard" src="https://github.com/driftyco/ionic-plugin-keyboard.git" version="1.0.8" />
    <access origin="beta.fimex.com.tw" />
    <vs:plugin name="cordova-plugin-whitelist" src="https://github.com/apache/cordova-plugin-whitelist.git" version="1.2.2-dev" />
</widget>
